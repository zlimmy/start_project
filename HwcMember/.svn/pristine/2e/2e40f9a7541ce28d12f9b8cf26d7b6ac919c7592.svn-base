<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <com.hwc.member.widget.header.LXHeadView
        xmlns:my="http://schemas.android.com/apk/res/com.hwc.member"
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        my:centertitle="扫码结果" />
    
    	<View style="@style/divider_horizontal" android:layout_marginTop="20dp"/>

    	<RelativeLayout
    	    android:id="@+id/captur_rl"
    	    android:layout_width="match_parent"
    	    android:layout_height="150dp" 
    	    android:background="@color/white"
    	    >

    		<TextView
    		    android:id="@+id/result_tv"
    		    android:layout_width="wrap_content"
    		    android:layout_height="wrap_content"
    		    android:layout_alignParentLeft="true"
    		    android:textColor="@color/gray5"
    		    android:layout_marginTop="10dp"
    		    android:layout_marginLeft="10dp"
    		    android:textSize="@dimen/text_16"
    		     />   
    	</RelativeLayout>
    	
    	<View style="@style/divider_horizontal"/>
    	
    	
    	<RelativeLayout
         android:layout_width="match_parent"
         android:layout_height="wrap_content" 
         android:layout_marginTop="40dp">
        
      <Button 
        android:id="@+id/do_bt"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_centerHorizontal="true"
        style="@style/btn_tv_style"
        android:text="访问地址" />
      
     </RelativeLayout>
</LinearLayout>

package com.huimodel.api.response;

import java.io.Serializable;
import java.util.List;

import com.huimodel.api.base.DShop;
import com.huimodel.api.base.ResponseBase;

/**
 * @author FZ
 */
public class ShopAndShopProductInfoResponse extends ResponseBase implements Serializable {

	private static final long serialVersionUID = 1L;

	private List<DShop> shops;

	private int total;

	public List<DShop> getShops() {
		return shops;
	}

	public void setShops(List<DShop> shops) {
		this.shops = shops;
	}

	public int getTotal() {
		return total;
	}

	public void setTotal(int total) {
		this.total = total;
	}

}
package com.huimodel.api.request;

import com.huimodel.api.base.RequestBase;

public class PromLotteryDrawGetRequest extends RequestBase {
	
	private String lottery_cat;
	private String prom_begin;
	private String prom_end;
	private String status;
	
	//默认保留N条
	private int limit=5;

	public int getLimit() {
			return limit;
	}

	public void setLimit(int limit) {
			this.limit = limit;
	}

	public String getLottery_cat() {
		return lottery_cat;
	}

	public void setLottery_cat(String lottery_cat) {
		this.lottery_cat = lottery_cat;
	}

	public String getProm_begin() {
		return prom_begin;
	}

	public void setProm_begin(String prom_begin) {
		this.prom_begin = prom_begin;
	}

	public String getProm_end() {
		return prom_end;
	}

	public void setProm_end(String prom_end) {
		this.prom_end = prom_end;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}
	
	
}
